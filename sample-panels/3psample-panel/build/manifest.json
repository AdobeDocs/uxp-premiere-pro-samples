{
  "id": "com.adobe.dva.thirdparty",
  "shortname": "3PPANEL",
  "name": "Scripting Plugin",
  "version": "1.0.0",
  "main": "index.html",
  "host": [
    {
      "app": "premierepro",
      "minVersion": "24.0.0"
    }
  ],
  "manifestVersion": 5,
  "requiredPermissions": {
    "localFileSystem": "request"
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "mainPanel",
      "minimumSize": {
        "width": 230,
        "height": 200
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 230,
        "height": 300
      },
      "preferredFloatingSize": {
        "width": 400,
        "height": 300
      },
      "label": {
        "default": "Scripting Plugin"
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark.png",
          "scale": [1, 2],
          "theme": ["darkest", "dark", "medium"]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/light.png",
          "scale": [1, 2],
          "theme": ["lightest", "light"]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icons/plugin-icon.png",
      "scale": [1, 2],
      "theme": ["darkest", "dark", "medium", "lightest", "light", "all"],
      "species": ["pluginList"]
    }
  ]
}
